// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "./src/generated/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Car {
  id        Int      @id @default(autoincrement())
  vin       String
  make      String
  model     String
  year      Int
}

model Appointment {
  id        Int      @id @default(autoincrement())
  carId     Int
  date      DateTime
}
